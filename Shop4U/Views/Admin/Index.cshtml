@{
    ViewData["Title"] = "Admin Page";
}



    <form class="form-signin">
        <h1 class="h3 mb-3 font-weight-bolder">ממשק ניהול</h1>
        </br>

        <label for="inputEmail" class="sr-only">כתובת מייל</label>
        <input type="email" id="inputEmail" class="form-control" placeholder="כתובת מייל" required autofocus>
        <label for="inputPassword" class="sr-only">סיסמא</label>
        <input type="password" id="inputPassword" class="form-control" placeholder="סיסמא" required>
     

        </br>
        <button class="btn btn-lg btn-primary btn-block font-weight-bolder" type="submit">התחבר</button>
    </form>
